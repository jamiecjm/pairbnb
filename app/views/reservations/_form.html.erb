<%=form_for reserve do |form|%>
	<div class="form-group">
	  <%= form.label :start_date %>
	  <%= form.text_field :start_date,id: "datepicker",class: "form-control" %>
	</div>

	<div class="form-group">
	  <%= form.label :end_date %>
	  <%= form.text_field :end_date,id: "datepicker2",class: "form-control" %>
	</div>

	<div class="form-group">
	  <%= form.hidden_field :listing_id, value: session[:listing_id]%>
	</div>

	<%=form.submit "Book",class: "btn btn-default"%>
<%end%>

